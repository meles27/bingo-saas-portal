###
# LIST GAME-SESSIONS
GET {{ TENANT_BASE_URL }}/game-sessions HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# CREATE GAME-SESSION
POST {{ TENANT_BASE_URL }}/game-sessions HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json

{
  "gameId": "{{ gameId }}",
  "branchId": "{{ branchId }}",
  "totalRounds": 3,
  "entryFee": "300"
}


###
# RETRIEVE GAME-SESSION
GET {{ TENANT_BASE_URL }}/game-sessions/{{ sessionId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# UPDATE GAME-SESSION
PATCH {{ TENANT_BASE_URL }}/game-sessions/{{ sessionId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json

{
  "name": "Friday Night Mega Prize Bingo"
}


###
# DELETE GAME-SESSION 
DELETE {{ TENANT_BASE_URL }}/game-sessions/{{ sessionId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json