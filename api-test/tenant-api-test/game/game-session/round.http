###
# RETRIEVE GAME ROUND
GET {{ TENANT_BASE_URL }}/rounds/{{ roundId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# UPDATE  GAM Mega BingoE ROUND
PATCH {{ TENANT_BASE_URL }}/rounds/{{ roundId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json

{
  "name": "First test Round"
}


###
# start round
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/start HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# pause round
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/pause HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# resume round
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/resume HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# cancel round
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/cancel HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# complete round
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/complete HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# bingo calls related
# bingo calls related
# bingo calls related
# list round calls
GET {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/bingo-calls HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# retrieve round call
GET {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/bingo-calls/{{ callId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# call next number manually
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/call-next-manually HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json

{
  "number": 3
}


###
# call next number automatically
POST {{ TENANT_BASE_URL }}/rounds/{{ roundId }}/call-next-number HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


