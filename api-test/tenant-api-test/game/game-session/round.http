###
# this is for the rounds in the single game
# LIST GAME ROUNDS
GET {{ TENANT_BASE_URL }}/games/{{ gameId }}/rounds HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# CREATE GAME ROUND
POST {{ TENANT_BASE_URL }}/games/{{ gameId }}/rounds HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json

