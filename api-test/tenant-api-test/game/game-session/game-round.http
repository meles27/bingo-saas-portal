###
# this is for the rounds in the single game session
# LIST GAME-SESSION ROUNDS
GET {{ TENANT_BASE_URL }}/game-sessions/{{ sessionId }}/rounds?search= HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# CREATE GAME-SESSION ROUND
POST {{ TENANT_BASE_URL }}/game-sessions/{{ sessionId }}/rounds HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json

{
  "prize": "500000",
  "configId": "{{ roundConfigId }}"
}


###
# RETRIEVE GAME-SESSION ROUND
GET {{ TENANT_BASE_URL }}/game-sessions//{{ sessionId }}/rounds/{{ roundId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# UPDATE  GAME-SESSION ROUND
PATCH {{ TENANT_BASE_URL }}/game-sessions//{{ sessionId }}/rounds/{{ roundId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json


###
# DELETE  GAME-SESSION ROUND
DELETE {{ TENANT_BASE_URL }}/game-sessions//{{ sessionId }}/rounds/{{ roundId }} HTTP/1.1
Authorization: Bearer {{ access }}
Content-Type: application/json